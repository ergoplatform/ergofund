{
  "name": "ergofund",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist/bin/www",
    "scheduler": "node ./dist/bree",
    "dev": "NODE_ENV=development npm-run-all build --parallel server scheduler",
    "prod": "NODE_ENV=production npm-run-all build --parallel server scheduler",
    "transpile": "babel ./src --out-dir dist/",
    "clean": "rimraf dist",
    "watch:dev": "nodemon"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "package.json",
      "config.js"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "@ladjs/graceful": "^1.0.5",
    "axios": "^0.24.0",
    "bree": "^6.4.0",
    "cabin": "^9.0.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "redis": "^4.0.0-rc.4",
    "rimraf": "^3.0.2",
    "sequelize": "^6.8.0",
    "signale": "^1.4.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "are-you-es5": "^2.1.2",
    "nodemon": "^2.0.14",
    "sequelize-cli": "^6.2.0"
  }
}
